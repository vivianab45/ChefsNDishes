@{
    ViewData["Title"]="All Chefs";
}

@model List<Chef>
<h1 class="text-center">Check out our roster of chefs!</h1>


<table class="table table-striped table-bordered">
    <thead>
        <th>Name</th>
        <th>Age</th>
        <th># of Dishes</th>
    </thead>
    <tbody>

            @foreach(Chef c in Model)
            {
                <tr>
                    <td>@c.FirstName @c.LastName</td>
                    <td>@c.Age</td>
                    <td>@c.AllDishes.Count</td>
                </tr>
            }

    </tbody>
</table>